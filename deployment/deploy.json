{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virusTotalApiKey": {
            "type": "string",
            "metadata": {
                "description": "virus total api key"
            }
        },
        "cdsOrgUniqueName": {
            "type": "string",
            "metadata": {
                "description": "Enter cds org unique name"
            }
        }
    },
    "functions": [],
    "variables": {
      "baseTemplatePath": "[deployment().properties.templateLink.uri]",
      "blobStorageAccount": "[take(concat('durinstorage',uniqueString(resourceGroup().id)),24)]",
      "durinTopicWebhookAppName": "[take(concat('durintopicwebhook',uniqueString(resourceGroup().id)),60)]",
      "eventHubName": "[take(concat('durinadfeventhub',uniqueString(resourceGroup().id)),60)]"
    },
  "resources": [
    {
      "name": "commondataservice",
      "type": "Microsoft.Web/connections",
      "apiVersion": "2016-06-01",
      "location": "[resourceGroup().location]",
      "tags": {
      },
      "properties": {
        "displayName": "example@microsoft.com",
        "parameterValues": {
        },
        "customParameterValues": {
        },
        "nonSecretParameterValues": {
        },
        "api": {
          "id": "[concat(subscription().id, '/providers/Microsoft.Web/locations/', resourceGroup().location, '/managedApis/commondataservice')]"
        }
      }
    },
    {
      "name": "virustotal",
      "type": "Microsoft.Web/connections",
      "apiVersion": "2016-06-01",
      "location": "[resourceGroup().location]",
      "tags": {
      },
      "properties": {
        "displayName": "virustotal",
        "parameterValues": {
        },
        "customParameterValues": {
          "x-api_key": "[parameters('virusTotalApiKey')]"
        },
        "nonSecretParameterValues": {
        },
        "api": {
          "id": "[concat(subscription().id, '/providers/Microsoft.Web/locations/', resourceGroup().location, '/managedApis/virustotal')]"
        }
      }
    },
    {
      "name": "office365",
      "type": "Microsoft.Web/connections",
      "apiVersion": "2016-06-01",
      "location": "[resourceGroup().location]",
      "tags": {
      },
      "properties": {
        "displayName": "",
        "parameterValues": {
        },
        "customParameterValues": {
        },
        "api": {
          "id": "[concat(subscription().id, '/providers/Microsoft.Web/locations/', resourceGroup().location, '/managedApis/office365')]"
        }
      }
    },
    {
      "name": "storage",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2019-10-01",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[uri(variables('baseTemplatePath'), 'storageaccount/deploy.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "storagename": {
            "value": "[variables('blobStorageAccount')]"
          }
        }
      },
      "dependsOn": [
        "[resourceId('Microsoft.Web/connections', 'commondataservice')]"
      ]
    },
    {
      "name": "azurefunctiondeployment",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2019-10-01",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[uri(variables('baseTemplatePath'), 'azure-function/deploy.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "blobStorageName": {
            "value": "[variables('blobStorageAccount')]"
          },
          "durinTopicWebhookAppName": {
            "value": "[variables('durinTopicWebhookAppName')]"
          }
        }
      },
      "dependsOn": [
        "[resourceId('Microsoft.Resources/deployments', 'storage')]"
      ]
    },
    {
      "name": "eventgrid-topic-deployments",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2019-10-01",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[uri(variables('baseTemplatePath'), 'storageaccount/eventgrid.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "storagename": {
            "value": "[variables('blobStorageAccount')]"
          },
          "durinTopicWebhookAppName": {
            "value": "[variables('durinTopicWebhookAppName')]"
          }
        }
      },
      "dependsOn": [
        "[resourceId('Microsoft.Resources/deployments', 'storage')]",
        "[resourceId('Microsoft.Resources/deployments', 'azurefunctiondeployment')]"
      ]
    },
    {
      "name": "logiapps-deployment",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2019-10-01",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[uri(variables('baseTemplatePath'), 'logicapps/deploy.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "baseTemplatePath": { "value": "[variables('baseTemplatePath')]" },
          "crmOrgName": { "value": "[parameters('cdsOrgUniqueName')]" },
          "storageName": { "value": "[variables('blobStorageAccount')]" },
          "eventHubName": { "value": "[variables('eventHubName')]" }
        }
      },
      "dependsOn": [
        "[resourceId('Microsoft.Resources/deployments', 'storage')]",
        "[resourceId('Microsoft.Resources/deployments', 'azurefunctiondeployment')]",
        "[resourceId('Microsoft.Resources/deployments', 'eventgrid-topic-deployments')]",
        "[resourceId('Microsoft.Resources/deployments', 'eventhub-deployment')]"
      ]
    },
    {
      "name": "CONNECTION-CONFIGURATION",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2019-10-01",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[uri(variables('baseTemplatePath'), 'logicapps/output.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
        }
      },
      "dependsOn": [
        "[resourceId('Microsoft.Resources/deployments', 'logiapps-deployment')]"
      ]
    },
    {
      "name": "eventhub-deployment",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2019-10-01",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[uri(variables('baseTemplatePath'), 'event-hub/deploy.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "eventHubName": {
            "value": "[variables('eventHubName')]"
          }
        }
      },
      "dependsOn": []
    }
  ],
    "outputs": {
    }
}