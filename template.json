{
    "$schema": "https://schema.management.azure.com/schemas/2014-04-01-preview/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "logicAppApiVersion": {
            "defaultValue":"2016-06-01",
            "type": "String"
        },
        "workflowName": {
            "type": "String",
            "defaultValue":"getAccounts"
        },
        "workflowLocation": {
            "type": "String",
            "allowedValues": [
            "[resourceGroup().location]",
            "australiaeast",
            "australiasoutheast",
            "brazilsouth",
            "centralus",
            "eastasia",
            "eastus",
            "eastus2",
            "japaneast",
            "japanwest",
            "northcentralus",
            "northeurope",
            "southcentralus",
            "southeastasia",
            "westeurope",
            "westus"
        ]
        },
        "workflowSchema": {
            "type": "String",
            "defaultValue":"https://schema.management.azure.com/providers/Microsoft.Logic/schemas/2016-06-01/workflowdefinition.json"
        },
        "logicAppState": {
            "defaultValue": "Enabled",
            "type": "String"
        },
        "definition": {
            "defaultValue": "[concat('{\"contentVersion\":\"1.0.0.0\",\"parameters\":{},\"actions\":{},\"triggers\":{},\"outputs\":{},\"$schema\":\"', parameters('workflowSchema'), '\"}')]",
            "type": "String"
        },
        "parameters": {
            "defaultValue": {},
            "type": "Object"
        },
        "tags": {
            "defaultValue": {},
            "type": "Object"
        }
    },
    "resources": [
        {
            "type": "Microsoft.Logic/workflows",
            "apiVersion": "[parameters('logicAppApiVersion')]",
            "name": "[parameters('workflowName')]",
            "location": "[parameters('workflowLocation')]",
            "tags": "[parameters('tags')]",
            "properties": {
                "definition": "[json(parameters('definition'))]",
                "parameters": "[parameters('parameters')]",
                "state": "[parameters('logicAppState')]"
            }
        }
    ]
}